{
	"name": "DS_ADLS_File_Dynamic",
	"properties": {
		"linkedServiceName": {
			"referenceName": "LS_ADLS_GEN2",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FileName": {
				"type": "string",
				"defaultValue": "supermarket_sales.csv"
			},
			"FolderName": {
				"type": "string"
			}
		},
		"folder": {
			"name": "ADLS_container"
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",

			},
			{
				"name": "Item_Visibility",
				"type": "String"
			},
			{
				"name": "Item_Type",
				"type": "String"
			},
			{
				"name": "Item_MRP",
				"type": "String"
			},
			{
				"name": "Outlet_Identifier",
				"type": "String"
			},
			{
				"name": "Outlet_Establishment_Year",
				"type": "String"
			},
			{
				"name": "Outlet_Size",
				"type": "String"
			},
			{
				"name": "Outlet_Location_Type",
				"type": "String"
			},
			{
				"name": "Outlet_Type",
				"type": "String"
			},
			{
				"name": "Item_Outlet_Sales",
				"type": "String"
			}
		]
	}
}